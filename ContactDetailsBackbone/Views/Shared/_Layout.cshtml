	@Scripts.Render("~/bundles/backbone")
	@Scripts.Render("~/bundles/model")
	@Scripts.Render("~/bundles/collection")
	@Scripts.Render("~/bundles/templates")	
	@Scripts.Render("~/bundles/bootstrap")
